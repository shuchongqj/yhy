<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:background="@color/color_norm_ffffff"
              android:gravity="center"
              android:orientation="vertical"
              android:paddingLeft="@dimen/dd_dimen_24px"
              android:paddingRight="@dimen/dd_dimen_24px">

    <ImageView
        android:id="@+id/cell_person_info_club_head"
        android:layout_width="@dimen/dd_dimen_82px"
        android:layout_height="@dimen/dd_dimen_82px"/>

    <TextView
        android:id="@+id/cell_person_info_club_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dd_dimen_20px"
        android:singleLine="true"
        android:textColor="@color/tv_color_gray6"
        android:textSize="@dimen/dd_dimen_24px"/>
</LinearLayout>