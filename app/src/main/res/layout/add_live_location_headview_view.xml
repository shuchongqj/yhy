<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:id="@+id/add_live_loation_nosearch"
    android:paddingBottom="@dimen/dd_dimen_40px"
    android:paddingTop="@dimen/dd_dimen_40px"
    android:background="@color/color_norm_ffffff"
    >
    <TextView
        android:layout_marginLeft="@dimen/dd_dimen_30px"
        android:id="@+id/add_live_loation_search_new_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="不显示位置"
        android:textColor="@color/main"
        android:textSize="@dimen/dd_dimen_30px"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />



    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/icon_search_tick"
        android:layout_centerVertical="true"
        android:layout_marginRight="@dimen/dd_dimen_30px"
        android:layout_alignParentRight="true"/>

</RelativeLayout>