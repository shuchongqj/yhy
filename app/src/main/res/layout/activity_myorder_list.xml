<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.quanyan.yhy.ui.discovery.view.SimpleViewPagerIndicator
        android:id="@+id/order_tab_bar"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dd_dimen_88px"
        android:background="#ffffffff" />

    <View
        android:id="@+id/tem_line_overview_left_line"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dd_dimen_1px"
        android:background="#e1e1e1" />

    <android.support.v4.view.ViewPager
        android:id="@+id/viewpager"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:flipInterval="30"
        android:persistentDrawingCache="animation" />

    <FrameLayout
        android:id="@+id/frame_layout_order_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"></FrameLayout>
</LinearLayout>