<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
       android:layout_width="match_parent"
       android:layout_height="wrap_content">

    <com.quanyan.base.view.customview.NoScrollGridView
        android:id="@+id/noscroll_gridview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/dd_dimen_10px"
        android:paddingTop="@dimen/dd_dimen_10px"
        android:verticalSpacing="@dimen/dd_dimen_10px"
        android:gravity="center"
        android:horizontalSpacing="@dimen/dd_dimen_5px"
        android:stretchMode="columnWidth"
        android:cacheColorHint="#00000000"
        android:listSelector="#00000000"
        android:layout_marginLeft="@dimen/dd_dimen_10px"
        android:layout_marginRight="@dimen/dd_dimen_10px">
    </com.quanyan.base.view.customview.NoScrollGridView>
</merge>