<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:visibility="gone"
    android:background="@color/color_norm_ffffff"
    android:orientation="vertical">


    <RelativeLayout
        android:id="@+id/home_listview_rl_vip"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dd_dimen_30px"
        android:layout_marginRight="@dimen/dd_dimen_30px"
        android:layout_marginTop="@dimen/dd_dimen_50px">


        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:gravity="center"
            android:text="服务内容"
            android:textColor="@color/color_norm_636363"
            android:textSize="@dimen/dd_dimen_30px" />


    </RelativeLayout>


    <TextView
        android:id="@+id/cf_tv_servicecontent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="@dimen/dd_dimen_30px"
        android:layout_marginRight="@dimen/dd_dimen_30px"
        android:layout_marginTop="@dimen/dd_dimen_20px"
        android:lineSpacingExtra="@dimen/dd_dimen_10px"
        android:text="未知"
        android:textColor="@color/tab_title_normal"
        android:textSize="@dimen/dd_dimen_26px" />


    <TextView
        android:id="@+id/tlhc_title_check"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/dd_dimen_50px"
        android:background="@drawable/shape_more_item_rl_bg"
        android:padding="5dp"
        android:text="查看更多"
        android:visibility="gone"
        android:textColor="@color/neu_666666"
        android:textSize="@dimen/dd_dimen_30px" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dd_dimen_20px"
        android:layout_marginTop="@dimen/dd_dimen_40px"

        android:background="@color/color_norm_f4f4f4"
        android:orientation="vertical">

    </LinearLayout>


</LinearLayout>
